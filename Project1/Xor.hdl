/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    Nand(a=a, b=a, out=nota);
    Nand(a=b, b=b, out=notb);
    Nand(a=a, b=notb, out=anotb);
    Nand(a=nota, b=b, out=bnota);
    Nand(a=anotb, b=bnota, out=out);


}
